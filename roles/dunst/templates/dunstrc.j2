[global]
    ### Display ###
    monitor = 0
    geometry = "330-15+49"
    indicate_hidden = yes
    shrink = no
    transparency = 12
    notification_height = 100
    separator_height = 2
    padding = 10
    horizontal_padding = 10
    frame_width = 0
    separator_color = auto
    sort = no
    idle_threshold = 0

    ### Text ###
    font = DejaVu 12
    line_height = 3
    markup = full
    format = "<b>%s</b>\n%b"
    alignment = left
    show_age_threshold = -1
    word_wrap = yes
    ignore_newline = no
    stack_duplicates = false
    hide_duplicate_count = false
    show_indicators = yes

    ### Icons ###
    icon_position = left
    max_icon_size = 80
    # icon paths generation:
    # for theme in Papirus Papirus Adwaita hicolor; do for size in 48x48 48x48 scalable; do for type in actions animations apps categories devices emblems emotes mimetypes notifications places status; do f=/usr/share/icons/$theme/$size/$type; [ -d "$f" ] && echo -n ${f}: ; done; done; done; echo
