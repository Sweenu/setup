    ### History ###
    sticky_history = yes
    history_length = 20

    ### Misc/Advanced ###
    dmenu = /usr/bin/rofi -dmenu
    browser = /usr/bin/{{ browser }} -new-tab
    always_run_script = true
    title = Dunst
    class = Dunst
    startup_notification = false

    ### Legacy
    force_xinerama = false

[experimental]
    per_monitor_dpi = false

[shortcuts]
    close = ctrl+space
    close_all = ctrl+shift+space
    history = ctrl+period
    context = ctrl+shift+period

[urgency_low]
    frame_color = "#3B7C87"
    foreground = "#FFFFFF"
    background = "#000000"
    timeout = 7

[urgency_normal]
    frame_color = "#5B8234"
    foreground = "#FFFFFF"
    background = "#000000"
    timeout = 7

[urgency_critical]
    frame_color = "#B7472A"
    foreground = "#B7472A"
    background = "#191311"
    timeout = 7

[keep-transient]
    match_transient = true
    history_ignore = false

[bitwarden]
    summary = "*Bitwarden*"
    format = ""
